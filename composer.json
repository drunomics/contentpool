{
  "name": "drunomics/contentpool",
  "description": "The contentpool distribution",
  "type": "drupal-profile",
  "authors": [
    {
      "name": "Wolfgang Ziegler",
      "email": "fago@wolfgangziegler.net"
    },
    {
      "name": "drunomics GmbH",
      "email": "hello@drunomics.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GPL-2.0-or-later",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-8.x-1.x": "1.0.x-dev"
    },
    "patches": {
      "drupal/core": {
        "#1356276-417: Allow profiles to provide a base/parent profile and load them in the correct order": "https://www.drupal.org/files/issues/drupal-n1356276-417-d8.5.%2A.patch",
        "#2893029: StringTranslationTrait consumers cannot be reliably serialized without DependencySerializationTrait": "https://www.drupal.org/files/issues/2893029-full-fix-test-from-3.patch"
      },
      "drupal/custom_elements": {
        "#2994894-5 Extend preprocessor to cope with thunder paragraphs.": "https://www.drupal.org/files/issues/2018-08-29/2994894-5.patch"
      },
      "drupal/workspace": {
        "Removes workspace forms from toolbar to avoid disappearing.": "https://gist.githubusercontent.com/archnode/113e781c3673cacc76e2b8ea9fa47eea/raw/221b9b67f8f7d510883dede8943cb18a12c87b96/gistfile1.txt"
      }
    }
  },
  "require": {
    "burdamagazinorg/thunder": "^8.2.16",
    "cweagans/composer-patches": "~1.0",
    "drupal/default_content": "^1.0@alpha",
    "drupal/default_content_extra": "^1.0@beta",
    "drupal/file_entity": "^2.0@beta",
    "drunomics/contentpool_replication": "^1.0.3",
    "drupal/relaxed": "^1.0@beta",
    "drunomics/service-utils": "*",
    "drupal/workspace": "^1.0@beta",
    "drupal/custom_elements": "^1.0@beta",
    "drupal/trusted_redirect": "^1.3",
    "drupal/vuejs": "^1.0@beta"
  }
}
